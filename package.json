{
  "name": "react-router-node-starter",
  "version": "1.1.0",
  "description": "an example for deploying a React Router + NodeJS app to Heroku",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server --hot --inline",
    "lint": "eslint src/**",
    "lint:watch": "esw -w src/**",
    "start": "npm run build && NODE_ENV=production node server.js",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.spec.js --require ignore-styles",
    "test:watch": "npm test -- --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "expect": "^1.20.2",
    "express": "^4.15.4",
    "google-map-react": "^0.34.0",
    "google-maps-react": "^1.1.2",
    "node-sass": "^4.5.3",
    "points-cluster": "^0.1.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-date-picker": "^6.10.4",
    "react-datepicker": "^1.5.0",
    "react-day-picker": "^7.1.9",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.2.9",
    "react-geolocated": "^2.3.0",
    "react-google-maps": "^9.4.5",
    "react-motion": "^0.5.2",
    "react-router-dom": "^4.1.2",
    "react-spinkit": "^3.0.0",
    "sass-loader": "^6.0.6",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.78.3",
    "style-loader": "^0.18.2",
    "superagent": "^3.8.3",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.4.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "eslint-watch": "^3.1.2",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.5.0",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
